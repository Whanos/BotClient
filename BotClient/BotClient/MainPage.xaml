<Window x:Class="BotClient.MainPage"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:BotClient"
        mc:Ignorable="d"
        Title="MainPage" Height="450" Width="800">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="199*"/>
            <ColumnDefinition Width="601*"/>
        </Grid.ColumnDefinitions>
        <TextBox x:Name="textBox" HorizontalAlignment="Left" Text="Logging In..." TextWrapping="Wrap" VerticalAlignment="Top" Width="120" Margin="119,10,0,0" Grid.Column="1"/>
        <ComboBox x:Name="serverList" HorizontalAlignment="Left" Margin="54,98,0,0" VerticalAlignment="Top" Width="120">
        </ComboBox>
        <ComboBox x:Name="channelList" Grid.Column="1" HorizontalAlignment="Left" Margin="66,98,0,0" VerticalAlignment="Top" Width="120"/>

    </Grid>
</Window>
